<table>
    <tr>
        <td>Age</td>
        <td><%= result._age %></td>
    </tr>
    <tr>
        <td>Gender</td>
        <td>
        <% if (result._donGen == 0){%>
            Male            
        <% }else if (result._donGen == 1){%>
            Female
        <% }else {%>
            Other
        <% }%>
            
        </td>
    </tr>
    <tr>
        <td>Place</td>
        <td><%=result._place %></td>
    </tr>
    <tr>
        <td>Blood Group</td>
        <td>
            <% switch(result._grp) {
             case '0':%>
                 A+ve
            <% break;
             case '1':%>
                 A-ve
            <% break;
             case '2':%>
                 B+ve
            <% break;
             case '3':%>
                 B-ve
            <% break;
             case '4':%>
                 O+ve
            <% break;
            case '5':%>
                 O-ve
            <% break;
            case '6':%>
                 AB+ve
            <% break;
            case '7':%>
                 AB-ve
            <% } %>
        </td>
    </tr>
    <tr>
        <td>Medical Conditions</td>
        <td>
            <%if (result._medCond == true){%>
                Yes
            <% }else{ %>
                No
            <%}%>
        </td>
    </tr>
    <tr>
        <td>Availability/Status</td>
        <td>
            <%if (result._donorStatus == true) {%>
                Not Available
            <% }else{ %>
                Available
            <%}%>
        </td>
    </tr>
    <tr>
        <td>Token</td>
        <td>
            <%= result.token %>
        </td>
    </tr>
</table>